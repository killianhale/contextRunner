{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "NlogContextRunner": {
    "EnableContextEndMessage": true,
    "SuppressChildContextEndMessages": true,
    "ContextStartMessageLevel": "Debug",
    "SuppressContextByNameList": [
      "WeatherForecast_Get"
    ],
    "SanitizedProperties": [ "date" ]
  }
}
