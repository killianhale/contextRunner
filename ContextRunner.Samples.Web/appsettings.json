{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug"
    }
  },
  "AllowedHosts": "*",
  "NlogContextRunner": {
    "EnableContextStartMessage": true,
    "EnableContextEndMessage": true,
    "SuppressChildContextEndMessages": true,
    "ContextStartMessageLevel": "Debug",
    "ContextEndMessageLevel": "Debug",
    "SuppressContextByNameList": [
      "WeatherForecast_Get2"
    ],
    "SanitizedProperties": [
      "date"
    ],
    "MemoryTargetLogName": "memory"
  },
  "ContextRunnerHttp": {
    "PrintLogLineForHttpErrorCodes": true,
    "PrintLogLineForHttpSuccessCodes": true,
    "HttpSuccessCodeMessageLogLevel": "Debug"
  },
  "FlatIOContextRunner": {
    "LogDir": "./logs",
    "BaseLogName": "test"
  }
}
