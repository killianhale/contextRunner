{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "NlogContextRunner": {
    "EnableContextStartMessage": true,
    "EnableContextEndMessage": true,
    "SuppressChildContextStartMessages": true,
    "SuppressChildContextEndMessages": true,
    "ContextStartMessageLevel": "Info",
    "ContextEndMessageLevel": "Info",
    "ContextErrorMessageLevel": "Fatal",
    "SanitizedProperties": [ "date" ]
  }
}
